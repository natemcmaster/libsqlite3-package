{
    "name": "SQLite",
    "version": "3.12.3-*",
    "copyright": "Public Domain",
    "description": "SQLite is a software library that implements a self-contained, serverless, zero-configuration, transactional SQL database engine.",
    "authors": [
        "SQLite Development Team"
    ],
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "https://github.com/aspnet/libsqlite3-package"
        },
        "tags": [
            "SQLite",
            "embedded",
            "database",
            "sql"
        ],
        "files": {
            "mappings": {
                "lib/netstandard1.0/": "lib/netstandard1.0/*"
            }
        }
    },
    "frameworks": {
        "netstandard1.0": {
            "dependencies": {
                "SQLite.Native": "3.12.3-*"
            }
        },
        "uap10.0": {
            "dependencies": {
                "SQLite.UWP.Native": "3.12.3-*"
            }
        }
    }
}