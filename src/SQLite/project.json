{
    "name": "SQLite",
    "version": "3.12.2-*",
    "copyright": "Public Domain",
    "description": "SQLite is a software library that implements a self-contained, serverless, zero-configuration, transactional SQL database engine. This package contains native libraries for SQLite on Univeral Windows Platform apps.",
    "authors": [
        "SQLite Development Team"
    ],
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "https://github.com/aspnet/libsqlite3-package"
        },
        "tags": [
            "SQLite",
            "embedded",
            "database",
            "sql"
        ]
    },
    "frameworks": {
        "netstandard1.0": {
            "dependencies": {
                "SQLite.Native": "3.12.2-*"
            }
        },
        "uap10.0": {
            "dependencies": {
                "SQLite.UWP.Native": "3.12.2-*"
            }
        }
    }
}